<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #166392;" stylesheets="@../css/mainWindow.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.CarsAndOwnersController">
 <top>
    <HBox prefHeight="51.0" prefWidth="592.0" BorderPane.alignment="CENTER">
       <children>
          <Label alignment="CENTER" prefHeight="26.0" prefWidth="60.0" stylesheets="@../css/mainWindow.css" text="Tulaj neve">
             <HBox.margin>
                <Insets left="5.0" top="10.0" />
             </HBox.margin>
          </Label>
          <TextField fx:id="searchField" prefHeight="29.0" prefWidth="155.0" stylesheets="@../css/mainWindow.css">
             <padding>
                <Insets left="20.0" />
             </padding>
             <HBox.margin>
                <Insets left="5.0" top="10.0" />
             </HBox.margin>
          </TextField>
            <Button mnemonicParsing="false" onMouseClicked="#searchOwner" prefHeight="29.0" stylesheets="@../css/mainWindow.css" text="Keres">
               <HBox.margin>
                  <Insets left="5.0" top="10.0" />
               </HBox.margin>
            </Button>
       </children>
    </HBox>
 </top>
   <center>
    <ListView fx:id="listOfCars" prefHeight="200.0" prefWidth="200.0" stylesheets="@../css/mainWindow.css" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets left="5.0" right="5.0" />
         </BorderPane.margin></ListView>
   </center>
   <bottom>
      <AnchorPane prefHeight="42.0" prefWidth="600.0" BorderPane.alignment="CENTER" />
   </bottom>
   <left>
      <ListView fx:id="listOfOwners" prefHeight="307.0" prefWidth="270.0" stylesheets="@../css/mainWindow.css" BorderPane.alignment="CENTER" />
   </left>
</BorderPane>
